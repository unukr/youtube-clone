header.header
    .header__wrapper
        .header__column
            a(href=routes.home)
                i.fab.fa-youtube
                    span YouTube
        .header__column
            form(action=routes.search, method="get")
                input(type="text", placeholder="검색", name="term")
        .header__column
            ul
                if loggedUser
                    li
                        a(href=`${routes.videos}${routes.upload}`) Upload
                    li
                        a(href=routes.me) Profile
                    li
                        a(href=routes.logout) Log Out
                else
                    li
                        a(href=routes.join) Join
                    li
                        a(href=routes.login) Log In